     letExp(
      decList(
       varDec(arr1,
        arrtype,
        arrayExp(arrtype,
         intExp(10),
         intExp(0)),
        TRUE),
       decList(
        typeDec(
         nametyList(
          namety(arrtype,
           arrayTy(int)),
          nametyList())),
        decList())),
      seqExp(
       expList(
        varExp(
         simpleVar(arr1)),
        expList())))